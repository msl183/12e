                "detour": "proxy"
            },
            {
                "tag": "local",
                "type": "local",
                "detour": "direct"
            },
            {
                "type": "fakeip",
                "tag": "fakeip",
                "inet4_range": "198.18.0.0/15"
                //  "inet6_range": "fc00::/18"
            }
        ],
        "strategy": "ipv4_only",
        "rules": [
            {
                "rule_set": "geosite-category-ads-all",
                "action": "reject"
            },
            {
                "action": "route",
                "rule_set": "geosite-geolocation-cn",
                "server": "local"
            },
            {
                "query_type": [
                    "A"
                ],
                "server": "fakeip"
            }
        ]
    },
    "inbounds": [
        {
            "type": "tun",
            "tag": "tun-in",
            "interface_name": "tun0",
            "address": [
                "172.18.0.1/30"
            ],
            "auto_route": true,
         //   "auto_redirect": true,
            "strict_route": false,
            "stack": "mixed",
            "sniff": true,
            "sniff_override_destination": true
        }
    ],
    "outbounds": [
        //直连
        {
            "tag": "direct",
            "type": "direct"
                    },
        //代理
        {
            "type": "vless",
            "tag": "proxy",
            "server": "104.194.72.128",
            "server_port": 443,
            "uuid": "ab54f3fc-192d-4d2f-97fd-1a9dfe230ba7",
            "flow": "xtls-rprx-vision",
            // "network": "tcp",
            "tls": {
                "enabled": true,
                "server_name": "images.apple.com",
                "insecure": false,
                "utls": {
                    "enabled": true,
                    "fingerprint": "firefox"
                },
                "reality": {
                    "enabled": true,
                    "public_key": "txaEQ_ew-jUx7tSHhqPlmJ2zb18yWyRdPNvwWA2QuBc",
                    "short_id": "1a9dfe230ba7"
                }
            },
            "packet_encoding": "",
            "multiplex": {},
            "transport": {}
            // 拨号字段
        }
    ],
    //路由
    "route": {
        "auto_detect_interface": true,
       
        "final": "direct",
        "rules": [
            {
                "action": "sniff"
            },
            {
                "protocol": "dns",
                "action": "hijack-dns"
            },
            {
                "action": "route",
                "protocol": "bittorrent",
                "outbound": "direct"
            },
            {
                "action": "route",
                "rule_set": [
                    "geosite-geolocation-!cn",
                    "geoip-telegram"],
                "domain_keyword": [
                    "emby"
                ],
                "outbound": "proxy"
            },
           
            {
                "action": "reject",
                "rule_set": [
                    "geosite-category-ads-all"
                ]
            },
            {
                "action": "route",
                "rule_set": "geosite-geolocation-cn",
                "domain_keyword": [
                    "wogg",
                    "饭太硬"
                ],
                "domain_regex": [
                    ".*[一-龥].*"
                ],
                "outbound": "direct"
            },
             {
                "action": "route",
                "rule_set": "geoip-cn",
                "invert": true
                "outbound": "proxy"
            },
            {
                "action": "route",
                "ip_cidr": [
                    "192.168.1.0/24"
                ],
                "outbound": "direct"
            }
        ],
        "rule_set": [
            {
                "tag": "geosite-category-ads-all",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-category-ads-all.srs",
                "download_detour": "proxy"
            },
            {
                "tag": "geosite-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-cn.srs",
                "download_detour": "proxy"
            },
            {
                "tag": "geosite-geolocation-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-cn.srs",
                "download_detour": "proxy"
            },
            {
                "tag": "geoip-cn",
